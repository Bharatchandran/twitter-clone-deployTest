<%- include('../partials/header')%>
<div class="w-full max-w-3xl p-4 border border-gray-200 rounded-lg shadow  dark:bg-gray-800 dark:border-gray-700  ">
    
    
        <div class="flow-root">
            <ul role="list" class="divide-y divide-gray-200 dark:divide-gray-700">
                <% users.forEach(user => { %>
                <a href="/profiles/<%=user._id%>">
                <li class="py-3 sm:py-4 ">
                    <div class="flex items-center space-x-4 ml-4">
                        <div class="flex-shrink-0">
                            <img class="w-16 h-16 rounded-full" src="<%=user.avatar%>" alt="Neil image">
                        </div>
                        <div class="flex-1 min-w-0">
                            <p class=" text-xl font-medium text-gray-900 dark:text-white">
                                <%=user.name%>
                            </p>
                            <!-- <p class="text-sm text-gray-500 truncate dark:text-gray-400">
                               <%=user.email%>
                            </p> -->
                        </div>
                       
                    </div>
                </li>
                </a>
                <%}) %>
            </ul>

       </div>
   
    </div>

<%- include('../partials/footer')%>
